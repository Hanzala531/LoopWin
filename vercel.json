{
  "version": 2,
  "functions": {
    "index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}
